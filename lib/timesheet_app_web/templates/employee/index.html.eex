<h1>List of Employees</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <!-- Add other columns if needed -->
    </tr>
  </thead>
  <tbody>
    <%= for employee <- @employees do %>
      <tr>
        <td><%= employee.name %></td>
        <td><%= employee.email %></td>
        <!-- Add other columns if needed -->
      </tr>
    <% end %>
  </tbody>
</table>
